{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "blog_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "author_name": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Digital Marketing",
        "E-Commerce",
        "API",
        "Events & Conferences",
        "Startup Business",
        "Internships",
        "Other"
      ]
    },
    "date": {
      "type": "date"
    }
  }
}
